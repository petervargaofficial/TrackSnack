<h1>All Items:</h1>
<p> <%= link_to("create new item", new_menu_item_path) %> </p>


<table>
  <th>
    <td>Item name</td>
    <td>Description</td>
    <td>Price</td>
    <td>vendor_id</td>
    <td>Manage</td>
  </th>
  <% @menu_items.each do |item| %>
  <tr>
    <td><%= item.item_name %></td>
    <td><%= item.item_description %></td>
    <td><%= item.item_price %></td>
    <td><%= item.vendor_id %></td>
    <td>
      <%= link_to("detail", menu_item_path(item)) %>
      <%= link_to("edit", edit_menu_item_path(item)) %>
      <%= link_to("delete", item, :method => :delete, :confirm => "Are you sure?") %>
    </td>
  </tr>
  <% end %>
</table>